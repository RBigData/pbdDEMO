  note = "R Package, URL http://cran.r-project.org/package=pbdDEMO",
  textVersion = "Schmidt, D., Chen, W.-C., Ostrouchov, G., and Patel, P. (2013) pbdDEMO: Programming with Big Data -- Demonstrations of pbd Packages, URL http://cran.r-project.org/package=pbdDEMO",
  key = "Schmidt2013pbdDEMOpackage"
)

