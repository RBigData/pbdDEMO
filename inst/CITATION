  title = "pbdDEMO: Programming with Big Data --
           Demonstrations of pbd Packages",
  author = personList(as.person("Drew Schmidt"),
                      as.person("Wei-Chen Chen"),
                      as.person("George Ostrouchov"),
                      as.person("Pragneshkumar Patel")),
